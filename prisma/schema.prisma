generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @unique
  name       String
  email      String   @unique
  phone      String
  address    String
  pkg        String
  courses    String
  came_from  String
  start_date String
  end_date   String
}
